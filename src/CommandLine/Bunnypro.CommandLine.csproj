    <AssemblyName>Bunnypro.CommandLine</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
  </ItemGroup>

  <PropertyGroup>
    <PackageId>Bunnypro.CommandLine</PackageId>
    <PackageVersion>0.1.0</PackageVersion>
    <PackageDescription>A Command Line Library for Dotnet</PackageDescription>
    <Authors>Mubarrid Albar</Authors>
    <Owners>bunnypro</Owners>
    <PackageLicenseUrl>https://github.com/bunnypro/CommandLine.Net/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/bunnypro/CommandLine.Net</PackageProjectUrl>
    <PackageTags>command-line command-line-app command-line-application command-line-tools</PackageTags>
  </PropertyGroup>
</Project>
